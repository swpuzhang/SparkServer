{
  "urls": "http://*:6000",
  "WSHost": "http://127.0.0.1:10080/App",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {

        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "log/log.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 104857600,
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz}][{Level}][{ThreadId}][{ProcessId}] {Message:lj} {NewLine}{Exception}"
        }
      }
    ]
  },
  "Rabbitmq": {
    "Uri": "rabbitmq://localhost/SkyWatch/",
    "UserName": "SkyWatch",
    "Passwd": "sky_watch_2019_best",
    "Host": "localhost",
    "Vhost": "SkyWatch",
    "Queue": "WSGateWay-1"
  },

  "MongoSettings": {

    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "SkyWatch"
  },
  "Redis": {
    "ConnectionString": "localhost:6379"
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
